{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/server.js"
      },
      {
        "src": "/auth/start",
        "dest": "/server.js"
      },
      {
        "src": "/auth/callback",
        "dest": "/server.js"
      },
      {
        "src": "/logout",
        "dest": "/server.js"
      },
      {
        "src": "/jwks.json",
        "dest": "/server.js"
      }
    ]
  }
  